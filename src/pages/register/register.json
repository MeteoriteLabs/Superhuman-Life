                "type": "object",
                "properties": {
                    "specialist": {
                        "type": "string"
                    }
                }
            },
            "education": {
                "type": "array",
                "title": "Educational qualifications",
                "items": {
                    "type": "object",
                    "properties": {
                        "instituteName": {
                            "title": "Institue Name",
                            "type": "string"
                        },
                        "specialization":{
                            "title": "Specialization",
                            "type": "string"
                        },
                        "yearOfPassing":{
                            "title": "Year of Passing",
                            "type": "number"
                        },
                        "typeOfDegree":{
                            "type": "string",
                            "title": "Type of Degree",
                            "enum": [
                                "Bachelors",
                                "Masters",
                                "Diploma",
                                "Certification"
