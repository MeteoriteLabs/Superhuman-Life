{
    "1": {
        "title": "Sign Up",
        "type": "object",
        "required": [
            "email",
            "password",
            "confirm"
        ],
        "properties": {
            "email": {
                "type": "string",
                "title": "Email",
                "format": "email"
            },
            "password": {
                "type": "string",
                "title": "Password",
                "minLength": 8
            },
            "confirm": {
                "type": "string",
                "title": "Confirm Password",
                "minLength": 8
            },
            "socialLogins": {
                "type": "string"
            }
        }
    },
    "2": {
        "title": "Sign Up",
        "type": "object",
        "required": [
            "fname",
            "dob",
            "gender",
            "contact",
            "lname",
            "userName"
        ],
        "properties": {
            "fname": {
                "type": "string",
                "title": "First Name"
            },
            "lname": {
                "type": "string",
                "title": "Last Name"
            },
            "userName": {
                "type": "string",
                "title": "User Name"
            },
            "dob": {
                "type": "string",
                "title": "Date of Birth",
                "format": "date"
            },
            "gender": {
                "type": "string",
                "title": "Gender",
                "enum": [
                    "Female",
                    "Male",
                    "Other"
                ]
            },
            "contact": {
                "type": "number",
                "title": "Contact",
                "minLength": 10
            },
            "location": {
                "type": "string",
                "title": "Location"
            },
            "timezone": {
                "type": "string",
                "title": "Timezone"
            }
        }
    },
