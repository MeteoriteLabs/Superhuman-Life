{
    "1": {
        "title": "",
        "type": "object",
        "required": ["packagename", "equipmentList"],
        "properties": {
            "packagename": {
                "type": "string",
                "title": "Name of Offering",
                "description": "Enter the Title for Offering"
            },
            "equipmentList": {
                "type": "string",
                "description": "Add all tools required"
            },
            "tags": {
                "type": "string",
                "title": "Hash tags",
                "description": "Use hashtags to improve your listing"
            }
        }
    },
    "2": {
        "title": "",
        "type": "object",
        "required": ["About", "Benifits", "thumbnail", "VideoUrl", "languages"],
        "properties": {
            "About": {
                "type": "string",
                "title": "About",
                "description": "Summarize the offering"
            },
            "Benifits": {
                "type": "string",
                "title": "Benefits",
                "description": "List the advantages"
            },
            "languages": {
                "type": "string"
            },
            "thumbnail": {
                "type": "string",
                "description": "This image will be seen in the listing screen"
            },
            "VideoUrl": {
                "type": "string",
                "format": "youtubeurl",
                "description": "Add Youtube URL"
            }
        }
    },
    "3": {
        "title": "Program Details",
        "type": "object",
        "required": ["programDetails"],
        "properties": {
            "programDetails": {
                "type": "string"
            }
        }
    },
    "4": {
        "title": "For Programs",
        "type": "object",
        "required": ["bookingleadday"],
        "properties": {
            "bookingleadday": {
                "type": "number",
                "description": "The lead time will allow you to discuss, understand, request data and then build a custom program.",
                "title": "Consumer can choose start date, after",
                "enum": [0, 1, 2, 3, 4, 5],
                "enumNames": ["1 day", "2 days", "3 days", "4 days", "5 days", "6 days"],
                "default": 0
            }
        }
    },
    "5": {
        "title": "Pricing Plan",
        "type": "object",
        "required": ["pricingDetail"],
        "properties": {
            "pricingDetail": {
                "type": "string",
                "title": "Pricing Plan",
                "value": ""
            }
        }
    },
    "6": {
        "title": "Package Settings",
        "type": "object",
        "required": ["visibility", "config"],
        "properties": {
            "visibility": {
                "type": "number",
                "title": "Visibility",
                "enum": [0, 1],
                "enumNames": ["Public", "Private"],
                "default": 0,
                "description": "Public: The Offering will be visible to the user as soon as it is created, Private: The offering will not ve seen by anyone until you share it with them"
            },
            "datesConfig": {
                "type": "string"
            },
            "config": {
                "title": "Booking Config",
                "type": "object",
                "required": ["bookingConfig"],
                "properties": {
                    "bookingConfig": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
