{
    "1": {
        "title": "",
        "type": "object",
        "required": ["name", "cuisine"],
        "properties": {
            "name": {
                "type": "string",
                "title": "Name of Package"
            },
            "cuisine": {
                "type": "string",
                "title": "Cuisine",
                "enum": ["Chinese", "Italian", "Mexican", "Thai"]
            },
            "tags": {
                "type": "string",
                "title": "Tags"
            },
            "chat": {
                "type": "string",
                "title": "Chat Access",
                "enum": ["Yes", "No"]
            }
        }
    },
    "2": {
        "title": "",
        "type": "object",
        "required": ["about", "benefits", "consult"],
        "properties": {
            "about": {
                "type": "string",
                "title": "About"
            },
            "benefits": {
                "type": "string",
                "title": "Benefits"
            },
            "consult": {
                "type": "string",
                "title": "Conuslt Type",
                "enum": ["Solo", "Couple", "Family"]
            },
            "video": {
                "type": "string",
                "format": "uri",
                "title": "Recipe Video Link"
            },
            "thumbnail": {
                "type": "string",
                "format": "data-url",
                "title": "Recipe Picture",
                "accept": ".jpg"
            }
        }
    },
    "3": {
        "title": "Program Details",
        "description": "Program to be Assigned after package purchase",
        "type": "object",
        "required": ["mode"],
        "properties": {
            "mode": {
                "type": "string",
                "title": "Mode",
                "enum": ["Online", "Offline", "Hybrid"]
            },
            "location": {
                "type": "string",
                "title": "Location",
                "enum": ["Others", "At My Address", "At Client Address"]
            },
            "pin": {
                "type": "string",
                "title": ""
            }
        },
        "dependencies": {
            "mode": {
                "oneOf": [
                    {
                        "properties": {
                            "mode": {
                                "enum": ["Online"]
                            },
                            "consult": {
                                "type": "object",
                                "title": "No of Consultations",
                                "properties": {
                                    "online": {
                                        "type": "number",
                                        "title": "Online"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "properties": {
                            "mode": {
                                "enum": ["Offline"]
                            },
                            "consult": {
                                "type": "object",
                                "title": "No of Consultations",
                                "properties": {
                                    "online": {
                                        "type": "number",
                                        "title": "Offline"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "properties": {
                            "mode": {
                                "enum": ["Hybrid"]
                            },
                            "consult": {
                                "type": "object",
                                "title": "No of Consultations",
                                "properties": {
                                    "online": {
                                        "type": "number",
                                        "title": "Online"
                                    },
                                    "offline": {
                                        "type": "number",
                                        "title": "Offline"
                                    }
                                }
                            }
                        }
                    }
                ]
            }
        }
    },
    "4": {
        "title": "",
        "type": "object",
        "properties": {
            "voucher": {
                "type": "string",
                "title": "Additional Voucher"
            },
            "plan": {
                "type": "string",
                "title": "Nutrition Plan",
                "enum": ["Monthly", "Quaterly", "Half Yearly", "Yearly"]
            },
            "price": {
                "type": "number",
                "title": "Set Pricing"
            }
        }
    }
}
